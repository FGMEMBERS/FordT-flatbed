<!-- controls, wheel and suspension animations -->

<!-- gear selector anim. -->

  <animation>
  <object-name>brakepedal</object-name>
  <type>rotate</type>
  <property>controls/gear/brake-left</property>
  <factor>23</factor>
  <center>
   <x-m>-0.7767</x-m>
   <y-m> 0.1322</y-m>
   <z-m>-0.1346</z-m>
  </center>
  <axis>
   <x>0</x>
   <y>-1</y>
   <z>0</z>
  </axis>
 </animation>

 <animation>
  <object-name>secondpedal</object-name>
  <type>rotate</type>
  <property>controls/gearstick-x</property>
  <factor>-1.2</factor>
  <center>
   <x-m>-0.7767</x-m>
   <y-m> 0.1322</y-m>
   <z-m>-0.1346</z-m>
  </center>
  <axis>
   <x>0</x>
   <y>-1</y>
   <z>0</z>
  </axis>
 </animation>

  <animation>
  <object-name>reversepedal</object-name>
  <type>rotate</type>
  <property>controls/gearstick-y</property>
  <factor>-1.4</factor>
  <center>
   <x-m>-0.7767</x-m>
   <y-m> 0.1816</y-m>
   <z-m>-0.1346</z-m>
  </center>
  <axis>
   <x>0</x>
   <y>-1</y>
   <z>0</z>
  </axis>
 </animation>

 <!-- handbrake -->
 <animation>
  <object-name>emergbrake</object-name>
  <type>rotate</type>
  <property>controls/gear/brake-parking</property>
  <factor>14</factor>
  <center>
   <x-m>-0.6921</x-m>
   <y-m> 0.0581</y-m>
   <z-m>-0.1195</z-m>
  </center>
  <axis>
   <x>0</x>
   <y>1</y>
   <z>0</z>
  </axis>
 </animation>

<!-- clutch lever function parking brake lever -->

 <animation>
  <object-name>emergbrake</object-name>
  <type>rotate</type>
  <property>engines/engine/clutch</property>
  <factor>-14</factor>
  <center>
   <x-m>-0.6921</x-m>
   <y-m> 0.0581</y-m>
   <z-m>-0.1195</z-m>
  </center>
  <axis>
   <x>0</x>
   <y>1</y>
   <z>0</z>
  </axis>
 </animation>

<!-- throttle lever -->

<animation>
  <type>rotate</type>
  <object-name>throttle</object-name>
  <property>/controls/flight/elevator</property>
  <factor>-73</factor><axis>
   <x1-m>-0.5239</x1-m>
   <y1-m>0.26902</y1-m>
   <z1-m>0.5814</z1-m>
   <x2-m>-0.9998</x2-m>
   <y2-m>0.2723</y2-m>
   <z2-m>0.12283 </z2-m>
  </axis>
 </animation>

<!-- steering -->
  <animation>
    <type>rotate</type>
    <object-name>steeringwheel</object-name>
    <property>controls/flight/aileron</property>
    <factor>350</factor>
  <axis>
   <x1-m>-0.47003</x1-m>
   <y1-m>0.3015</y1-m>
   <z1-m>0.6343</z1-m>
   <x2-m>-0.8248</x2-m>
   <y2-m>0.3015</y2-m>
   <z2-m>0.2892 </z2-m>
  </axis>
  </animation>

<!-- Front Axle -->
 <animation>
  <type>translate</type>
  <object-name>leftfrontwhl</object-name>
  <object-name>rightfrontwhl</object-name>
  <object-name>leftsteerknuckle</object-name>
  <object-name>rightsteerknuckle</object-name>
  <object-name>frontaxle</object-name>
  <property>gear/gear[0]/compression-norm</property>
    <condition>
        <greater-than-equals>
          <property>gear/gear[1]/compression-norm</property>
          <property>gear/gear[0]/compression-norm</property>
        </greater-than-equals>
    </condition>
  <interpolation>
   <entry>
    <ind>0.0</ind>
    <dep>-0.005</dep>
   </entry>
   <entry>
    <ind>1.00</ind>
    <dep>0.055</dep>
   </entry>
  </interpolation>
